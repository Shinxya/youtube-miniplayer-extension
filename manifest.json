{
    "manifest_version": 2,
    "name": "youtube-miniplayer",
    "version": "1.0",
    "description": "show mini youtube player",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "exclude_matches": ["https://www.youtube.com/*"],
            "js": ["js/appendPlayer.js"]
        }
    ]
}